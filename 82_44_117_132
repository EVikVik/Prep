def uniqueness(in_str):
    set_str = ""
    for letter in in_str:
        if letter in set_str:
            return letter
        else:
            set_str += letter
    return None

test_str = "Elaena"

result = uniqueness(test_str)

if result:
    print("This letter appear twice:", result)
else:
    print("All letters are unique!")
