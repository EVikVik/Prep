import os

def find_duplicates(path):
    duplicates = {}
    print("Exist:", os.path.isdir(path))
    for root, dirs, files in os.walk(path, True):
        print("root:", root)
        print("dirs:", dirs)
        print("files:", files)
        for file in files:
            print("file:", file)
            if file.endswith(".rtf"):
                add_to_duplicates(duplicates, os.path.join(root, file))

    return duplicates
            
def add_to_duplicates(duplicates, file):
    size = os.path.getsize(file)
    print("size:", size)
    if duplicates.get(size):
        duplicates[size].append(file)
    else:
        duplicates[size] = [file]
        
path = "/Users/kutanov/test"

print(find_duplicates(path))
